var callbacks = { 
	renderSubmitPaper: function ($m){	// render 'Submit Paper' either as a button or as a menu
		$m.attr('id', 'submit-paper');
		if ($m.next('ul').find('li').length>0) {
			$m.addClass('expander');
			$m.after('<a class="expander mobileOnly" href="#" onclick="mobileMenuExpand(this);"></a>')
		}
		else {
			$m.parent().addClass('redButton');
		}
	},
	renderSubmisionGuidelines: function ($m){
		$m.each(function (i, v){
			let parentMenu=$(v).closest('.primaryNav>li').find('a.expander');
			//console.debug(parentMenu);
			if (parentMenu!==undefined && parentMenu.length>0){
				switch($(parentMenu[0]).attr('data-item-name')){
					case 'view-journal-info': $(v).attr('data-item-name', 'view-submission-guidelines'); break;
					case 'view-submit-paper': $(v).attr('data-item-name', 'view-submit-guidelines'); break;
					default: console.debug('Unexpected menu item parent: ', parentMenu.attr('data-item-name')); break;
				};
			}
			else
				console.debug('Parent menu not found for: ', $(v).text());
		})
	},
	renderOtoTps: function ($m){
		try{
			href=$m.attr('href');
			href=href.split('?')[0]+'?sagelogin='+encodeURIComponent(window.location.href);
			$m.attr('href', '#');
			$m.addClass('externalLink');
			$m.attr("onclick", "window.location = '"+ href +"';");
		}catch(e) {}
	},
	pushToDataLayer: function ($m){
		try{
			if (dataLayer && dataLayer.length>0) {
				pushToGTM('journal-nav', 'journal-header', 'NV', 'view', $m.attr('data-item-name'), 6);
			}
			else {
				console.debug('Data Layer not ready, yet.');
				setTimeout(function() {callbacks['pushToDataLayer']($m);}, 500);
			}
		} catch(e) {}
	}
};
function addMenuAttributes() {
	menuAttr['items'].forEach(function(value) {
		let $m=$('.journalMenus .primaryNav li>a[data-item-name="'+value["menu-name"]+'"]');
		if ($m.length){
			$m.attr('data-item-name', value["data-item-name"]);
			if (value["callback"]) {
				callbacks[value["callback"]]($m);
			}
			else {
				if (value["class"]){
					$m.addClass(value["class"]);
					if (value["class"].indexOf('activeLink')!==-1)
						$m.after('<a class="expander mobileOnly" href="#" onclick="mobileMenuExpand(this);"></a>')
				} 
				//if (value["parent-class"]) $m.parent().addClass(value["parent-class"]);
				if (value["href"]) $m.attr('href', value["href"]);
				if (value["id"]) $m.attr('id', value["id"]);
			}
		}
	});
}
function mobileMenuExpand(el){
	$(el).siblings('a.expander').toggleClass('expanded');
}
function restoreExpanderLinks(){
	$('a[data-item-name="view-journal-info"]').mousedown(function(){
		$(this).attr('href', '/journal-info/ijr');
	});
	$('a[data-item-name="view-stay-connected"]').mousedown(function(){
		$(this).attr('href', '/connected/ijr');
	});
}
function addSubmenus(){
	$('div.journal-submenu').each(function() {
		$(this).find('div>li').insertBefore($(this));
	});
}
function singleSubmenuItem(){
	$('.single-item a').each(function() {
		let $href=$(this).closest('li').next('.single-item-href').find('a');
		$(this).attr('href',  $href.attr('href'));
		if ($href.hasClass('externalLink')) $(this).addClass('externalLink');
	});
}
function checkNoSubmenus(){
	$('a.expander').each(function() {
		if ($(this).closest('li').find('ul>li').length==0){
			$(this).removeClass('expander activeLink');
			$(this).next('a.mobileOnly').hide();
		}
	});
}
addMenuAttributes();
addSubmenus();
restoreExpanderLinks();
singleSubmenuItem();
checkNoSubmenus();
