<div class="widget-body body body-none  body-compact-all"><script>
$('#share-email #showMailPage input[name="title"]').attr('value', $('<textarea />').html('Optimal path planning for surveillance with temporal-logic constraints').text().replace(/<\/?[^>]+(>|$)/g, ""));
$('#share-email #emailColleague')[0].innerHTML=$('#share-email #emailColleague')[0].innerHTML.replace('The e-mail addresses that you supply to use this service will not be used for any other purpose without your consent.','');
$('#share-email #showMailPage input[type="submit"]').attr('data-item-name', 'share-email');

function initMailTab() {
	pushToGTM('share-menu', 'floating', 'UF', 'send', 'share-email', '6');
	if ($('#share-email #ok-rv').length===0)
		$('#share-email').prepend('<p id="ok-rv">Your email has been sent!</p>');
	if ($('#share-email #emailColleague .noteLabel').length===0)
		$('#share-email #emailColleague').prepend('<p class="noteLabel">All fields are required</p>');
	if ($('#share-email #showMailPage .shareCancelButton').length===0){
		$('#share-email #showMailPage').append('<div class="shareCancelButton smallButton blueGrayButton"><a class="shareCancelButtonLink" onclick="articleToolsToggle()" href="javascript:;"><span>Cancel</span></a></div>');
		// send mail via AJAX from the new share dialog (tabbed)
		$('#share-email #showMailPage input[type="submit"]').click(function(e) {
			e.preventDefault();
			if (validateForm())
				sendMail();
			return false;
		});
	}
}
function validateForm() {
	$('#share-email .error').remove();
	let mailTo=$('#share-email #mailTo').val();
	if (mailTo.length===0 || !isMailAddress(mailTo))
		$('#share-email #mailTo').before('<div class="validation error">You have to enter a valid email address here</div>');
	let mailFrom=$('#share-email #mailFrom').val();
	if (mailFrom.length===0 || !isMailAddress(mailFrom))
		$('#share-email #mailFrom').before('<div class="validation error">You have to enter a valid email address here</div>');
	if ($('#share-email #subject').val().length===0)
		$('#share-email #subject').before('<div class="validation error">You have to enter the email subject</div>');
	if ($('#share-email #name').val().length===0)
		$('#share-email #name').before('<div class="validation error">You have to enter your name</div>');
	return $('#share-email .error').length===0;
}
function sendMail() {
	$.ajax({
		url:$('#share-email #showMailPage').attr('action'),
		type:'post',
		data:$('#share-email #showMailPage').serialize()+'&submitForm=Send',
		success:function(result){
			console.debug('Request sent!');
			let errorText=$(result).find('#showMailPage .error:not(.validation)').text();
			if (errorText.length) {
				console.debug(errorText);
				if (errorText.indexOf('Captcha is invalid')!==-1)
					$('#sampleCaptcha_ReloadLink').click();
				if ($('#share-email .validationDiv #error-rv').length===0)
					$('#share-email .validationDiv').prepend('<div id="error-rv" class="error">'+errorText+'</div>');
				else
					$('#share-email .validationDiv #error-rv').text(errorText);
			} else {
				if ($(result).find('.pagefulltext p').text().indexOf('has been sent!')!==-1) {
					console.debug('Mail sent!');
					$('#share-email, #share-text').addClass('mail-sent');
					$('#sampleCaptcha_ReloadLink').click();
				}
				else {
					console.log('Mail NOT sent...');
				}
			}
		},
		error:function(xhr,status,error) {
			console.log('Request failed: ('+ status +') '+ error);
		}
	});
}
</script></div>
